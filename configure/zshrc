#export FZF_DEFAULT_COMMAND="find ./ -not -path \"*/coverage/*\" -not -path \"*/docs/*\"  -not -path \"*/.git/*\"  -not -path \"*/node_modules/*\" -not -path \"*/dist/*\" -type f -name '*'"

# 构建 find 命令
ignored_dirs=("node_modules" "dist" "docs" ".git")
exclude_conditions=()
for dir in "${ignored_dirs[@]}"; do
    exclude_conditions+=(-path \"*/$dir/*\" -prune -o)
done
# 强制忽略.git，顺便兼容多一个-o的问题
#find_command="find . \( "${exclude_conditions[@]}" -type d -name \".git\" -prune \) -o -type f -name '*'"
find_command="find . \( "${exclude_conditions[@]}" -type f -name \"core.*\" -prune \) -o -type f -print"
export FZF_DEFAULT_COMMAND=$find_command

# TSW 需要的环境变量
