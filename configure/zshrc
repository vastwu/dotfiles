# 内网代理
function use-proxy() {
  export {http,https,ftp,all}_proxy=http://127.0.0.1:12639
  echo $http_proxy
}
function unset-proxy() {
  unset {http,https,ftp,all}_proxy
}
function mnet() {
  ssh vastwu@pcg.mnet2.com -p 36000
}

